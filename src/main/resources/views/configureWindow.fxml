<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="294.0" prefWidth="565.0" styleClass="mainFxmlClass, lightboxBackground" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sk44.jfxgallery.controllers.ConfigureWindowController">
  <children>
    <HBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <BorderPane maxHeight="150.0" prefHeight="150.0" prefWidth="454.0" styleClass="lightboxForeground">
          <bottom>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="52.0" prefWidth="454.0" BorderPane.alignment="CENTER">
              <children>
                <Button mnemonicParsing="false" onAction="#handleSaveAction" text="SAVE">
                  <HBox.margin>
                    <Insets right="5.0" fx:id="x1" />
                  </HBox.margin>
                </Button>
                <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleCancelAction" text="CANCEL" />
              </children>
              <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x2" />
              </padding>
            </HBox>
          </bottom>
          <center>
            <GridPane alignment="TOP_LEFT" prefHeight="57.0" prefWidth="218.0">
              <children>
                <HBox alignment="CENTER_RIGHT" padding="$x1" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
                  <children>
                    <Label alignment="CENTER_RIGHT" contentDisplay="TOP" text="Startup Path" textAlignment="RIGHT">
                      <labelFor>
                        <TextField fx:id="pathField" prefWidth="305.0" />
                      </labelFor>
                    </Label>
                  </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                  <children>
                    <fx:reference source="pathField" />
                    <Button mnemonicParsing="false" onAction="#handleBrowseAction" text="..." />
                  </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.margin="$x1" GridPane.rowIndex="1">
                  <children>
                    <Label text="Viewr Mode">
                      <labelFor>
                        <ChoiceBox fx:id="viewerModeChoiceBox">
                          <items>
                            <FXCollections fx:factory="observableArrayList">
                              <String fx:value="Item 1" />
                              <String fx:value="Item 2" />
                              <String fx:value="Item 3" />
                            </FXCollections>
                          </items>
                        </ChoiceBox>
                      </labelFor>
                    </Label>
                  </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <children>
                    <ComboBox fx:id="viewerModeComboBox">
                      <items>
                        <FXCollections fx:factory="observableArrayList">
                          <String fx:value="Item 1" />
                          <String fx:value="Item 2" />
                          <String fx:value="Item 3" />
                        </FXCollections>
                      </items>
                    </ComboBox>
                  </children>
                </HBox>
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="221.0" minWidth="10.0" prefWidth="99.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="345.0" minWidth="10.0" prefWidth="345.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
              <BorderPane.margin>
                <Insets left="5.0" right="5.0" />
              </BorderPane.margin>
            </GridPane>
          </center>
          <stylesheets>
            <URL value="@style.css" />
          </stylesheets>
          <top>
            <HBox alignment="TOP_LEFT" minHeight="22.0" padding="$x2" prefHeight="41.0" prefWidth="454.0">
              <children>
                <Label styleClass="windowTitle" text="CONFIGURE" />
              </children>
            </HBox>
          </top>
        </BorderPane>
      </children>
    </HBox>
  </children>
  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>
</AnchorPane>
