<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" onKeyPressed="#handleKeyPressed" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass, lightboxBackground" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sk44.jfxgallery.ImageWindowController">
  <children>
    <BorderPane id="rootPane" fx:id="imageContainer" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button fx:id="buttonPrevious" mnemonicParsing="false" onAction="#handlePreviousAction" prefHeight="60.0" styleClass="navigation" text="〈" textAlignment="LEFT" />
      </children>
    </VBox>
    <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button fx:id="buttonNext" mnemonicParsing="false" onAction="#handleNextAction" prefHeight="60.0" styleClass="navigation" text="〉" textAlignment="RIGHT" />
      </children>
    </VBox>
    <HBox alignment="CENTER" prefHeight="28.0" prefWidth="600.0" styleClass="commandArea" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
      <children>
        <Label fx:id="imageNameLabel" contentDisplay="CENTER" styleClass="imageTitle" text="foobar.jpeg" textAlignment="CENTER">
          <HBox.margin>
            <Insets />
          </HBox.margin>
        </Label>
      </children>
    </HBox>
  </children>
  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>
</AnchorPane>
